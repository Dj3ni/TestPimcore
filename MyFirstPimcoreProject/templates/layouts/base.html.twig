<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        {% if pimcore_head_title().rawContent =='' %}
            {% do pimcore_head_title(document.title) %}
        {% endif %}
        {{ pimcore_head_title()}}

        {% do pimcore_head_meta(document.description, 'description') %}
        {{ pimcore_head_meta() }}

        {#{% do pimcore_head_link('/bundles/pimcoreadmin/favicon.ico', 'icon', 'image/x-icon') %}
        {{ pimcore_head_link() }} #}

        {% block stylesheets %}

            {# Bootstrap #}
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        
            {# Poppins font #}
            <link rel="preconnect" href="https://fonts.googleapis.com">
            <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
            <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            {# Bootstrap #}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>
    {% include "layouts/nav.html.twig" %}
        <main>
            {% block body %}{% endblock %}
        </main>
    </body>
</html>
